{% if current_page != 'index.md' %}
  {% set breadcrumbs %}
    {% include '@oe/breadcrumbs/breadcrumbs.twig' with {
      heading_level: 'h2',
      heading: 'Breadcrumbs',
      id: 'breadcrumbs',
      breadcrumbs: get_breadcrumbs(current_page),
    } only %}
  {% endset %}
  {% include '@oe/callout/callout.twig' with {
    content: breadcrumbs,
  } only %}
{% endif %}